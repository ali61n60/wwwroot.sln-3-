  <xs:element name="AdvertisementCommon" nillable="true" type="tns:AdvertisementCommon" />
  <xs:complexType name="Price">
    <xs:sequence>
      <xs:element minOccurs="0" name="PriceType" type="tns:PriceType" />
      <xs:element minOccurs="0" name="price" type="xs:decimal" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Price" nillable="true" type="tns:Price" />
  <xs:simpleType name="PriceType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ForSale" />
      <xs:enumeration value="ForExchange" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PriceType" nillable="true" type="tns:PriceType" />
  <xs:simpleType name="BodyStatus">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NoAccident" />
      <xs:enumeration value="OneAccident" />
      <xs:enumeration value="TwoAccident" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="BodyStatus" nillable="true" type="tns:BodyStatus" />
